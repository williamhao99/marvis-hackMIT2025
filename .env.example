# MentraOS Configuration
# Get your API key from: https://developer.mentra.com
MENTRAOS_API_KEY=your_mentra_api_key_here
PACKAGE_NAME=com.marvis.hackmit2025

# Anthropic Claude API
# Get your API key from: https://console.anthropic.com
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Cerebras API (for ultra-fast LLM inference)
# Get your API key from: https://cerebras.ai
CEREBRAS_API_KEY=your_cerebras_api_key_here

# SerpAPI (for Google Search integration)
# Get your API key from: https://serpapi.com
SERPAPI_KEY=your_serpapi_key_here

# Exa API (alternative search - optional)
EXA_API_KEY=your_exa_api_key_here

# AWS S3 Configuration (for data persistence)
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-east-1

# S3 Bucket Configuration
S3_BUCKET_NAME=your-bucket-name
S3_BUCKET_URL=https://your-bucket-name.s3.amazonaws.com

# Server Configuration
PORT=3000
NODE_ENV=development
